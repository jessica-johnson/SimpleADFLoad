{
	"name": "pipeline_Load_Encounters",
	"properties": {
		"activities": [
			{
				"name": "Copy Encounter",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"preCopyScript": "truncate table dbo.encounters"
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"ID": "ID",
							"START": "START",
							"STOP": "STOP",
							"PATIENT": "PATIENT",
							"CODE": "CODE",
							"DESCRIPTION": "DESCRIPTION",
							"COST": "COST",
							"REASONCODE": "REASONCODE",
							"REASONDESCRIPTION": "REASONDESCRIPTION"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AzureBlob_StageFiles",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable_Encounters",
						"type": "DatasetReference"
					}
				]
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}