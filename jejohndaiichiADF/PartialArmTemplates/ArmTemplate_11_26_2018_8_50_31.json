{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "jejohndaiichiADF"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline_Load_Encounters')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Encounter",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "SqlSink",
								"writeBatchSize": 10000,
								"preCopyScript": "truncate table dbo.encounters"
							},
							"enableStaging": false,
							"dataIntegrationUnits": 0,
							"translator": {
								"type": "TabularTranslator",
								"columnMappings": {
									"ID": "ID",
									"START": "START",
									"STOP": "STOP",
									"PATIENT": "PATIENT",
									"CODE": "CODE",
									"DESCRIPTION": "DESCRIPTION",
									"COST": "COST",
									"REASONCODE": "REASONCODE",
									"REASONDESCRIPTION": "REASONDESCRIPTION"
								}
							}
						},
						"inputs": [
							{
								"referenceName": "AzureBlob_StageFiles",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSqlTable_Encounters",
								"type": "DatasetReference"
							}
						]
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AzureBlob_StageFiles')]",
				"[concat(variables('factoryId'), '/datasets/AzureSqlTable_Encounters')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlob_StageFiles')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "jejohndaiichistorage",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlob",
				"structure": [
					{
						"name": "ID",
						"type": "String"
					},
					{
						"name": "START",
						"type": "DateTime"
					},
					{
						"name": "STOP",
						"type": "DateTime"
					},
					{
						"name": "PATIENT",
						"type": "String"
					},
					{
						"name": "CODE",
						"type": "Int32"
					},
					{
						"name": "DESCRIPTION",
						"type": "String"
					},
					{
						"name": "COST",
						"type": "Decimal"
					},
					{
						"name": "REASONCODE",
						"type": "String"
					},
					{
						"name": "REASONDESCRIPTION",
						"type": "String"
					}
				],
				"typeProperties": {
					"format": {
						"type": "TextFormat",
						"columnDelimiter": ",",
						"rowDelimiter": "",
						"nullValue": "\\N",
						"treatEmptyAsNull": true,
						"skipLineCount": 0,
						"firstRowAsHeader": true
					},
					"fileName": "encounters.csv",
					"folderPath": "stage-file"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlTable_Encounters')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "jejohndaiichidb",
					"type": "LinkedServiceReference"
				},
				"type": "AzureSqlTable",
				"structure": [
					{
						"name": "ID",
						"type": "String"
					},
					{
						"name": "START",
						"type": "DateTime"
					},
					{
						"name": "STOP",
						"type": "DateTime"
					},
					{
						"name": "PATIENT",
						"type": "String"
					},
					{
						"name": "CODE",
						"type": "Int32"
					},
					{
						"name": "DESCRIPTION",
						"type": "String"
					},
					{
						"name": "COST",
						"type": "Double"
					},
					{
						"name": "REASONCODE",
						"type": "String"
					},
					{
						"name": "REASONDESCRIPTION",
						"type": "String"
					}
				],
				"typeProperties": {
					"tableName": "[concat('[', 'dbo].[encounters]')]"
				}
			}
		}
	]
}